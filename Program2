package com.twg.coding;

import java.util.Scanner;

public class Program2 {
  public static void main(String args[]) {
	  int a;
	  Scanner s = new Scanner(System.in);
	  a=s.nextInt();
	  if(a%3==0 && a%11==0) {
		  System.out.println("yes divisible");
	  }else {
		  System.out.println("Not divisible");
	  }
  }
}
